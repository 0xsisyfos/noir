{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "test",
			"problemMatcher": [
				"$rustc"
			],
			"args": [
				"--workspace",
				"--locked",
				"--release"
			],
			"group": "build",
			"label": "cargo test --workspace --locked --release"
		},
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"args": [
				"--fix",
			],
			"group": {
				"kind": "build",
				
			},
			"label": "cargo clippy --fix"
		}
	]
}